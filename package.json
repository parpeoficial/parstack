{
    "name": "stackerjs",
    "description": "Stack for building Microservices",
    "keywords": [
        "framework",
        "stack",
        "orm",
        "http",
        "lib"
    ],
    "version": "1.1.19",
    "main": "lib/index.js",
    "bin": {
        "forklift": "forklift.js"
    },
    "types": "index.d.ts",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/parpeoficial/stackerjs"
    },
    "dependencies": {
        "body-parser": "^1.18.2",
        "cli-table": "^0.3.1",
        "express": "^4.16.3",
        "stackerjs-db": "^1.1.2",
        "stackerjs-http": "^1.1.0",
        "stackerjs-orm": "^1.1.3",
        "stackerjs-types": "^1.1.1",
        "stackerjs-utils": "^1.1.3",
        "stacktrace-js": "^2.0.0"
    },
    "devDependencies": {
        "@types/mocha": "^2.2.48",
        "babel-cli": "^6.26.0",
        "babel-core": "^6.26.0",
        "babel-preset-env": "^1.6.1",
        "babel-register": "^6.26.0",
        "chai": "^4.1.2",
        "mocha": "^4.1.0",
        "nyc": "^11.7.1",
        "stackerjs-db-mysql-adapter": "^1.1.0"
    },
    "nyc": {
        "exclude": [
            "lib",
            "test",
            "index.js"
        ],
        "require": [
            "babel-register",
            "babel-polyfill"
        ],
        "reporter": [
            "lcov",
            "text"
        ]
    },
    "scripts": {
        "precommit": "npm run lint",
        "lint": "eslint commands/ src/ test/ forklift.js --fix",
        "build": "./node_modules/.bin/babel src -d lib",
        "test": "NODE_ENV=dev ./node_modules/.bin/nyc ./node_modules/mocha/bin/_mocha"
    }
}